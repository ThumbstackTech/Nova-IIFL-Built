{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "tsBuildInfoFile": ".tsbuildinfo",
    "diagnostics": true,
    "pretty": true,
    "strictFunctionTypes": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/_error.js",
    "Redux/Reducer/Reducer.tsx",
    "Redux/Action/HomeCardAction.tsx",
    "Redux/Constant/HomeCardConstant.tsx",
    "Redux/Reducer/Reducer.tsx",
    "Redux/Reducer/Reducer.tsx",
    "Redux/Store.tsx"
  ],
  "exclude": ["node_modules"]
}
